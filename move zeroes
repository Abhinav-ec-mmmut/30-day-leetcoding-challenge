class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int i=0,j=0;
        while(j<nums.size()){
            if(nums[j]!=0){
                //swapping
                int t=nums[i];
                nums[i]=nums[j];
                nums[j]=t;
                i++;
                
            }
            j++;
        }
        
        
    }
};
